{
  "name": "@tizianoluziramos/tslproject",
  "version": "3.1.0",
  "description": "TypeScript in multiple lenguages",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "scripts": {
    "build-doc": "rimraf docs && typedoc --options compilers/main.json --out docs/docs-html && typedoc --options compilers/main.json --plugin typedoc-plugin-markdown --out docs/docs-md && typedoc --options compilers/main.json --plugin typedoc-plugin-mermaid --out docs/docs-mermaid",
    "dev": "ts-node src/index.ts ",
    "test": "vitest run && node compilers/vitest-teardown.js",
    "notificationTeardown": "node compilers/vitest-teardown.js",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@types/node-hid": "^1.3.4",
    "axios": "^1.7.9",
    "eslint": "^8.35.0",
    "iconv-lite": "^0.6.3",
    "inquirer": "^12.6.3",
    "lodash": "^4.17.21",
    "mammoth": "^1.9.0",
    "moment": "^2.30.1",
    "node-fetch": "^3.3.2",
    "node-hid": "^3.1.2",
    "node-notifier": "^10.0.1",
    "open": "^10.1.2",
    "os": "^0.1.1",
    "pdf-parse": "^1.1.1",
    "prettier": "^2.8.8",
    "ssh2-sftp-client": "^11.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3",
    "uuid": "^11.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tizianoluziramos/TypeScript-Lenguage-Proyect.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/iconv-lite": "^0.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.17.14",
    "@types/node-fetch": "^2.6.12",
    "@types/pdf-parse": "^1.1.4",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.5.0",
    "jsdom": "^26.1.0",
    "pdf-parse": "^1.1.1",
    "rimraf": "^6.0.1",
    "tsx": "^4.20.3",
    "typedoc": "^0.28.5",
    "typedoc-plugin-markdown": "^4.6.4",
    "typedoc-plugin-mermaid": "^1.12.0",
    "typedoc-plugin-pages": "^1.1.0",
    "typedoc-theme-hierarchy": "^6.0.0",
    "vitest": "^3.2.3"
  }
}
